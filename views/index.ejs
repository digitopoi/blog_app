<% include ./partials/header %>


<% blogs.forEach(function(blog){ %>
    <div class="ui main text container segment">
        <h2><%= blog.title %></h2>
        <img src="<%= blog.image %>">
        <span><%= blog.created %></span>
        <p><%= blog.body %></p>
    </div>
<% }); %>

<% include ./partials/footer %>